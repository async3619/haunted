{
    "name": "haunted",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "ttsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "clean": "rimraf dist",
        "start": "yarn clean && nest start",
        "dev": "yarn clean && ts-node ./tools/watch.ts",
        "debug": "yarn clean && nest start --debug --watch",
        "prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:e2e": "jest --config ./test/jest.config.e2e.ts",
        "coverage": "istanbul-merge --out ./coverage-merged/coverage-final.json ./coverage/coverage-final.json ./coverage-e2e/coverage-final.json",
        "postcoverage": "nyc report -t coverage-merged --report-dir coverage-merged --reporter=html --reporter=cobertura"
    },
    "dependencies": {
        "@apidevtools/json-schema-ref-parser": "^10.1.0",
        "@apollo/server": "^4.5.0",
        "@apollo/subgraph": "^2.4.0",
